# volume stuff
xf86audioraisevolume
	changevolume.sh high

xf86audiolowervolume
	changevolume.sh low

xf86audiomute
	changevolume.sh muted

#custom scripts
super + print
	wscreenshot.sh

Super + Shift + return
  alacritty

super + shift + p
  powermenu.sh

# media keybinds
xf86audioplay
  playerctl play-pause

xf86audioprev
  playerctl previous

xf86audionext
  playerctl next

xf86audiostop
  playerctl stop

