super + shift + Return
  $TERMINAL

super + p
  dmenu_run

XF86MonBrightness{Up,Down}
  python ~/.local/bin/brightness.py -{inc,dec}

XF86Audio{Mute,RaiseVolume,LowerVolume}
  ~/.local/bin/changevolume.sh {muted,high,low}

XF86AudioMicMute
  mpv --vid=no --really-quiet /usr/share/sounds/freedesktop/stereo/audio-volume-change.oga & pactl set-source-mute @DEFAULT_SOURCE@ toggle

XF86Calculator
  mate-calc

XF86Audio{Prev,Play,Next}
  playerctl {previous,play-pause,next}

super + Print
  ~/.local/bin/xscreenshot.sh

super + g
  firefox

super + z
  kitty -e lf
